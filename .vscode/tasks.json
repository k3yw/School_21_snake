{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc-7 build active file",
            "command": "/usr/bin/gcc-7",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-Wall", 
                "-Werror", 
                "-Wextra", 
                "-Wno-override-init", 
                "-Wno-unused-parameter",
                "gameengine.c",
                "renderengine.c",
                "darray.c",
                "darraycell.c",
                "-lncurses",
                "-lcurses"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }, 
